<div class="page-content"><div class="row"><div class="col-xs-12" style="height:390px"><table class="table table-striped table-bordered table-hover"><thead><tr><th class="center"><input type="checkbox" class="ace par"><span class="lbl"></span></th><th data-i18n="task.targetContent"></th><th data-i18n="task.credit"></th><th data-i18n="task.targetNum"></th><th data-i18n="task.currentNum"></th><th data-i18n="task.status"></th></tr></thead><tbody id="table"></tbody></table></div><div class="col-xs-6"><div class="dataTables_info" id="dynamic-table_info" role="status" aria-live="polite"><button data-i18n="[title]operation.add" class="btn btn-white btn-primary btn-bold" onclick="buttonAdd()"><span><i class="ace-icon fa fa-plus-circle bigger-120 primary"></i></span></button> <button data-i18n="[title]operation.edit" class="btn btn-white btn-purple btn-bold" onclick="buttonEdit()"><span><i class="ace-icon fa fa-edit bigger-120 purple"></i></span></button> <button data-i18n="[title]operation.delete" class="btn btn-white btn-warning btn-bold" onclick="buttonRemove()"><span><i class="ace-icon fa fa-trash-o bigger-120 warning"></i></span></button></div></div><div class="col-xs-6"><div class="dataTables_paginate paging_simple_numbers" id="tablePage"></div></div></div></div><textarea id="templateTable" style="display:none">

  {{#empty}}
  <tr>
    <td align="center" colspan="7" data-i18n="message.emptyRecord"></td>
  </tr>
  {{/empty}}

  {{#data}}
  <tr>
    <td class="center"><input value="{{task_id}}" type="checkbox" class="ace sub"><span class="lbl"></span></td>
    <td>{{platform}}:{{task_logic_name}}
      <a href="javascript:void(0);" class="blue" title="{{task_target_description}}"><i class="ace-icon fa fa-search bigger-10 yellow"></i></a>
    </td>
    <td>{{task_credit}}</td>
    <td>{{task_target_num}}</td>
    <td>{{task_current_num}}</td>
    
    <td>{{task_status}}</td>
  </tr>
  {{/data}}

</textarea><script type="text/javascript">function loadTable(o){nowPage=o?o:nowPage,jsonm("task/index.php"+nowPage,"#templateTable","#table")}function buttonAdd(){pbox("task_add")}function buttonEdit(){checkMID()&&pbox("task_edit")}function buttonSearch(){pbox("task_search")}function buttonRemove(){checkMID()&&mconfirm(t("message.remove"),function(){jsonp("task/remove.php?id="+mIDBox,function(o){mbox(o.msg),loadTable()})})}loadTable();</script>